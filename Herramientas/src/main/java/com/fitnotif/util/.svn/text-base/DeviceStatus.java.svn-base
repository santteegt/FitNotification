package com.fitnotif.util;

/**
 * Enum en la que se listan los posibles estados en los que puede estar un dispositivo
 * @author malgia
 * @version 1.0
 */
public enum DeviceStatus {
    NEW("NEW"),
    ACTIVE("ACT"),
    INACTIVE("INA"),
    DELETED("DEL");
    
    private String type;
    
    private DeviceStatus(String type){
        this.type=type;
    }
    
    public String getType(){
        return this.type;
    }
}
